{
  "root": true,
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:vue/recommended",
    "eslint:recommended"
  ],
  "plugins": ["vue", "prettier"],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "jest": true
  },
  "globals": {
    "document": true,
    "window": true,
    "navigator": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "lines-between-class-members": 0
  }
}
